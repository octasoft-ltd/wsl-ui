{
  "name": "wsl-ui",
  "private": true,
  "license": "BUSL-1.1",
  "version": "0.6.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "wdio run wdio.conf.ts",
    "test:e2e:verbose": "cross-env VERBOSE=1 wdio run wdio.conf.ts",
    "test:e2e:dev": "cross-env WSL_MOCK=1 wdio run wdio.conf.ts",
    "test:e2e:spec": "cross-env WSL_MOCK=1 wdio run wdio.conf.ts --spec",
    "screenshots": "cross-env WSL_MOCK=1 wdio run wdio.conf.ts --spec src/test/e2e/specs/screenshots.spec.ts",
    "screenshots:store": "cross-env WSL_MOCK=1 SCREENSHOT_WIDTH=1920 SCREENSHOT_HEIGHT=1080 wdio run wdio.conf.ts --spec src/test/e2e/specs/screenshots.spec.ts",
    "tauri:build": "tauri build",
    "tauri:build:debug": "tauri build --debug --no-bundle",
    "tauri:build:debugBundles": "tauri build --debug",
    "tauri:build:quick": "tauri build --bundles"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.9.1",
    "@tauri-apps/plugin-dialog": "^2.4.2",
    "@tauri-apps/plugin-fs": "^2.4.4",
    "@tauri-apps/plugin-log": "^2.7.1",
    "@tauri-apps/plugin-shell": "^2.3.3",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@tauri-apps/cli": "^2.9.6",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^3.2.4",
    "@wdio/cli": "^9.21.1",
    "@wdio/junit-reporter": "^9.21.0",
    "@wdio/local-runner": "^9.21.0",
    "@wdio/mocha-framework": "^9.21.0",
    "@wdio/spec-reporter": "^9.20.0",
    "@wdio/types": "^9.20.0",
    "cross-env": "^10.1.0",
    "jsdom": "^27.3.0",
    "postcss": "^8.4.47",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "^7.2.7",
    "vitest": "^3.2.4"
  }
}
